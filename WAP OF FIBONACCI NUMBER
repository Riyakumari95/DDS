#include <iostream>
using namespace std;

int fib (int num) {
    if (num ==0) {
        return 0;
    }
    if (num==1){
        return 1;
    }
    else{
        return fib (num-1) + fib (num-2);
    }
}
int main() {
    for (int i =0 ; i<5; i++) {
    cout << fib(i)<<" ";
}

    return 0;
} 

OUTPUT : 0 1 1 2 3 
